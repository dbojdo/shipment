{
    "name" : "webit/shipment",
    "type" : "library",
    "description": "Web-IT Shipment",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "webit/addressing": "dev-master",
        "webit/php-tools": "dev-master",
        "doctrine/cache": "^1.2",
        "doctrine/collections": "^1.2",
        "symfony/event-dispatcher": "^2.3|^3.0"
    },
    "require-dev": {
        "jms/serializer": "^1.0",
        "phpunit/phpunit": "^4.8|^5.0",
        "doctrine/dbal": "^2.3",
        "behat/behat": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Shipment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\Shipment\\Feature\\": "features/bootstrap"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "webit/php-tools": "JMS Serializer Support",
        "doctrine/dbal": "To Doctrine DBAL Types"
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-2.x": "2.x-dev",
            "dev-master": "3.x-dev"
        }
    }
}
